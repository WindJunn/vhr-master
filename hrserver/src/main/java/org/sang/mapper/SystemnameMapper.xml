<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.sang.mapper.SystemnameMapper">
    <select id="getAllSysName" resultType="org.sang.bean.SystemName">
        SELECT * FROM systemname
    </select>

    <update id="updateCategoryById" parameterType="org.sang.bean.SystemName">
        UPDATE systemname SET title=#{title},leftname=#{leftname} WHERE id=#{id}
    </update>

</mapper>